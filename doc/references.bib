@misc{web:website,
    title = {Website},
    key = {Website},
    urldate = {2013-10-15},
    note = {This is a note to the web:website reference.},
    url = {\url{http://www.example.com}},
    howpublished = {\url{http://www.example.com}},
}

% Article
% Required fields: author, title, journal, year.
% Optional fields: volume, number, pages, month, note.
@article{article:article,
    author    = {surname, firstname and surname, firstname},
    title     = {Tile of the article:article reference},
    journal   = {Journal},
    volume   = {Volume},
    pages    = {Pages},
    year      = {year},
    month    = {month},
    note     = {Note of the article:article reference},
}

% Unpublished
% Required fields: author, title, note.
% Optional fields: month, year.
@unpublished{unpublished:unpublished,
    author = {surname, firstname and surname, firstname},
    title = {Title of the unpublished:unpublished reference.},
    note = {This is a note of the unpublished:unpublished reference.},
    url = {\url{http://www.example.com}},
    year = {2013},
}

% Book
